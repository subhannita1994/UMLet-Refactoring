				<text><![CDATA[Start Date]]></text>
			</staticText>
			<staticText>
				<reportElement style="Arial_Bold" mode="Opaque" x="345" y="277" width="62" height="11" backcolor="#000000" forecolor="#ffffff"/>
				<textElement>
					<font/>
				</textElement>
				<text><![CDATA[End Date]]></text>
