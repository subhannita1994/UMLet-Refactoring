			</textField>
			<textField pattern="dd/MM/yyyy">
				<reportElement x="345" y="1" width="62" height="11"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression class="java.sql.Timestamp"><![CDATA[$F{EndTimestamp}]]></textFieldExpression>
			</textField>
		</band>
